<!--pages/account/register.wxml-->
<form bindsubmit="registerSubmit">
<view class="list-margin-top">
  <view class="item-input">
      <text>昵称</text>
      <input type="text" name="nickname" placeholder="请输入你的昵称"/>
  </view>
  <view class="item-input">
      <text>用户名</text>
      <input type="text" name="userUsername" placeholder="请输入你的用户名"/>
  </view>
  <view class="item-input">
      <text> 密码</text>
      <input type="text" name="userPassword" placeholder="请输入你的密码"/>
  </view>
  <view class="item-input">
      <text>手机号</text>
      <input type="text" name="userPhone" placeholder="请输入你的手机号"/>
  </view>
</view>
<text class="hint">以上信息请保持与真实信息一致，否则将会影响你的使用</text>
<view>
    <button formType="submit"  class="register" >注册
    </button>
</view>

</form>